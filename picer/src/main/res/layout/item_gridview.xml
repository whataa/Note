<?xml version="1.0" encoding="utf-8"?>
<io.github.whataa.picer.widget.SquareFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/item_girdview_pic"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <ImageView
        android:id="@+id/item_gridview_state"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right|top"
        android:padding="8dp"
        app:srcCompat="@drawable/ic_check_box_outline_blank_black_26dp" />
</io.github.whataa.picer.widget.SquareFrameLayout>